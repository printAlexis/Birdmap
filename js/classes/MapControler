class MapControler{
    constructor(studieAnimals){
        this.animalControler = [];
        studieAnimals.forEach(element => {
            this.animalControler.push(new AnimalControler(element))
        });
        this.displayAnimals();
    }
    
    displayAnimals(){
        animalControler.forEach(element => {
            element.addToMap();
        });
    }
}